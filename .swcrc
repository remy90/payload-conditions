{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "target": "esnext",
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "dts": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
        "react": {
          "runtime": "automatic"
        }
      }
  },
  "module": {
    "type": "es6"
  }
}